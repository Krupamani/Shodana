	loan_fromowner_interestfree = models.IntegerField()
	household_number = models.IntegerField()
	landowner_caste = models.ForeignKey(Caste)
	input_owner_pesticide = models.IntegerField()
	loan_fromowner_withinterest = models.IntegerField()
	tenurial_registration = models.ForeignKey(Registration)
	input_owner_fertiliser = models.IntegerField()
	tenurial_contracttype = models.ForeignKey(Contracttype)
	extent = models.IntegerField()
	qty_hay_byowner = models.IntegerField()
	furtherinterest_payment_rent = models.IntegerField()
	input_owner_electricity = models.IntegerField()
	annual_fixedrent_kind = models.ForeignKey(Kind)
	input_owner_manure = models.IntegerField()
	landowner_extent_landholding = models.IntegerField()
	landtype = models.ForeignKey(Landtype)
	tenurial_sincewhen = models.DateTimeField()
	landowner_occupation = models.ForeignKey(Occupation)
